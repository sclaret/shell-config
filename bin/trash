#! /usr/bin/env bash

ARCHIVE_DIR=~/scratch/archive/$(date "+%Y%m%dT%H%M%S")

for f in $@; do
  echo "Attempting to archive '$f'"
  if [[ -f $f ]] || [[ -d $f ]]; then
    mkdir -p $ARCHIVE_DIR
    mv $f $ARCHIVE_DIR/$(basename $f)
    echo "Archived: $f to $ARCHIVE_DIR/$(basename $f)"
  else
    echo "Archived: nothing" 
  fi
done
